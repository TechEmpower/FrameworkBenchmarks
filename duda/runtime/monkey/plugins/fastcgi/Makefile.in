all: monkey-fastcgi.so

include ../Make.common

CC	= @echo "  CC   $(_PATH)/$@"; $CC
CC_QUIET= @echo -n; $CC
CFLAGS	= $CFLAGS
LDFLAGS = $LDFLAGS
DEFS    = $DEFS
OBJECTS = fastcgi.o \
	protocol.o \
	chunk.o \
	request.o \
	fcgi_fd.o \
	fcgi_config.o \
	fcgi_context.o \
	fcgi_env.o

-include $(OBJECTS:.o=.d)

monkey-fastcgi.so: $(OBJECTS)
	$(CC) $(CFLAGS) $(LDFLAGS) $(DEFS) -shared -o $@ $^ -lc
