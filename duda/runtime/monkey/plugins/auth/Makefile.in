all: monkey-auth.so
include ../Make.common

CC	= @echo "  CC   $(_PATH)/$@"; $CC
CC_QUIET= @echo -n; $CC
CFLAGS	= $CFLAGS 
LDFLAGS = $LDFLAGS
DEFS    = $DEFS
OBJECTS = sha1.o base64.o conf.o auth.o

-include $(OBJECTS:.o=.d)

monkey-auth.so: $(OBJECTS)
	$(CC) $(CFLAGS) $(LDFLAGS) $(DEFS) -shared -o $@ $^ -lc
