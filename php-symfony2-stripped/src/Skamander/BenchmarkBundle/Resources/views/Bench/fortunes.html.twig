{% extends "SkamanderBenchmarkBundle::layout.html.twig" %}

{% block title "Fortunes" %}

{% block content %}
    <table>
        <tr>
            <th>id</th>
            <th>message</th>
        </tr>

        {% for fortune in fortunes %}
            <tr>
                <td>{{ fortune.id }}</td>
                <td>{{ fortune.message }}</td>
            </tr>
        {% endfor %}

    </table>
{% endblock %}
