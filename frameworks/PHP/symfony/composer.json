{
	"type" : "project",
	"license" : "proprietary",
	"require" : {
		"php": "^7.0.8",
		"doctrine/doctrine-bundle": "^1.6",
		"doctrine/orm": "^2.5",
		"symfony/console": "^3.4",
		"symfony/flex": "^1.0",
		"symfony/framework-bundle": "^3.4",
		"symfony/lts": "^3",
		"symfony/twig-bundle": "^3.4",
		"symfony/yaml": "^3.4",
		"twig/twig": "^1.0||^2.0"
	},
	"require-dev" : {
		"symfony/dotenv" : "^3.4"
	},
	"config" : {
		"preferred-install" : {
			"*" : "dist"
		},
		"sort-packages" : true
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\Tests\\" : "tests/"
		}
	},
	"conflict" : {
		"symfony/symfony" : "*"
	},
	"extra" : {
		"symfony" : {
			"id" : "01C0MBGZ184NXGJSE6HFTV8B43",
			"allow-contrib" : false
		}
	}
}
