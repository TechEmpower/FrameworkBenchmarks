{
  "name": "uwebsockets-express",
  "version": "0.0.0",
  "description": "Express API compatibility layer for uWebSockets.js",
  "main": "./build/index.js",
  "type": "module",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 18.15.0"
  },
  "scripts": {
