    "drizzle-kit": "^0.19.13",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "tsc-alias": "^1.8.7",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "drizzle-orm": "^0.28.2",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "postgres": "^3.3.5",
