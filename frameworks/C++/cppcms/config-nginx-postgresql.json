{
    "localization": {
        "locales": [ "en_US.UTF-8" ]
    },
    "service": {
        "api": "fastcgi",
        "socket": "/var/tmp/cppcms.sock",
        "backlog": 65535
    },
    "http": {
        "script": "/"
    },
    "gzip": {
        "enable": false
    },
    "app": {
        "db_connection_string": "postgresql:host=tfb-database;dbname=hello_world;user=benchmarkdbuser;password=benchmarkdbpass;@pool_size=10"
    },
    "cache": {
        "backend": "thread_shared",
        "limit": 10000
    }
}