{
    "listeners": {
        "*:8080": {
            "pass": "applications/blacksheep"
        }
    },
    "applications": {
        "blacksheep": {
            "type": "python",
            "path": "/blacksheep",
            "working_directory": "/blacksheep",
            "protocol": "asgi",
            "module": "app",
            "callable": "app",
            "processes": {
                "max": 56,
                "spare": 14,
                "idle_timeout": 3600
            }
        }
    },
    "access_log": "/dev/null"
}