<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence 
        http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd" version="2.0">

    <persistence-unit name="techempower">
        <provider>org.datanucleus.api.jpa.PersistenceProviderImpl</provider>
        <properties>
            <property name="datanucleus.ConnectionDriverName" value="org.hsqldb.jdbcDriver"/>
            <property name="datanucleus.ConnectionURL" value="jdbc:hsqldb:mem:exampleDb"/>
            <property name="datanucleus.ConnectionUserName" value="sa"/>
            <property name="datanucleus.ConnectionPassword" value=""/>
            <property name="datanucleus.connectionPoolingType" value="C3P0"/>
        </properties>
    </persistence-unit>
    
</persistence>